                                          </FlowStep>
                                        </FlowDecision.False>
                                      </FlowDecision>
                                    </FlowDecision.True>
                                    <FlowDecision.False>
                                      <FlowDecision x:Name="__ReferenceID4" Condition="[FirstStatus]" DisplayName="First status?" sap2010:WorkflowViewState.IdRef="FlowDecision_11">
                                        <FlowDecision.True>
                                          <FlowStep x:Name="__ReferenceID6" sap2010:WorkflowViewState.IdRef="FlowStep_31">
                                            <ui:WriteCsvFile Encoding="{x:Null}" AddHeaders="True" DataTable="[TransactionStatus]" Delimitator="Comma" DisplayName="Write CSV - status file" FilePath="[in_StatusFileName]" sap2010:WorkflowViewState.IdRef="WriteCsvFile_1" />
                                          </FlowStep>
                                        </FlowDecision.True>
