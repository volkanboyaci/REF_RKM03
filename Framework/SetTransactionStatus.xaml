                            </Assign.Value>
                          </Assign>
                          <Assign DisplayName="Assign Remarks" sap2010:WorkflowViewState.IdRef="Assign_26">
                            <Assign.To>
                              <OutArgument x:TypeArguments="x:String">[Remarks]</OutArgument>
                            </Assign.To>
                            <Assign.Value>
                              <InArgument x:TypeArguments="x:String">[String.Empty]</InArgument>
                            </Assign.Value>
                          </Assign>
                          <ui:AddLogFields DisplayName="Add Transaction log fields" sap2010:WorkflowViewState.IdRef="AddLogFields_3">
                            <ui:AddLogFields.Fields>
                              <InArgument x:TypeArguments="x:String" x:Key="logF_TransactionStatus">Success</InArgument>
                              <InArgument x:TypeArguments="x:String" x:Key="logF_TransactionNumber">[io_TransactionNumber.ToString]</InArgument>
                              <InArgument x:TypeArguments="x:String" x:Key="logF_TransactionID">[in_TransactionID]</InArgument>
                              <InArgument x:TypeArguments="x:String" x:Key="logF_TransactionField1">[in_TransactionField1]</InArgument>
                              <InArgument x:TypeArguments="x:String" x:Key="logF_TransactionField2">[in_TransactionField2]</InArgument>
                              <InArgument x:TypeArguments="x:String" x:Key="logF_TransactionExecutionTime">[TransactionExecutionTime]</InArgument>
                            </ui:AddLogFields.Fields>
                          </ui:AddLogFields>
                          <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_8" Level="Info" Message="[in_Config(&quot;LogMessage_Success&quot;).ToString]" />
                          <ui:RemoveLogFields DisplayName="Remove Transaction log fields" sap2010:WorkflowViewState.IdRef="RemoveLogFields_3">
                            <ui:RemoveLogFields.Fields>
                              <scg:List x:TypeArguments="InArgument(x:String)" Capacity="8">
                                <InArgument x:TypeArguments="x:String">logF_TransactionStatus</InArgument>
                                <InArgument x:TypeArguments="x:String">logF_TransactionNumber</InArgument>
                                <InArgument x:TypeArguments="x:String">logF_TransactionID</InArgument>
                                <InArgument x:TypeArguments="x:String">logF_TransactionField1</InArgument>
                                <InArgument x:TypeArguments="x:String">logF_TransactionField2</InArgument>
                                <InArgument x:TypeArguments="x:String">logF_TransactionExecutionTime</InArgument>
                              </scg:List>
                            </ui:RemoveLogFields.Fields>
                          </ui:RemoveLogFields>
                          <Assign sap2010:WorkflowViewState.IdRef="Assign_7">
                            <Assign.To>
                              <OutArgument x:TypeArguments="x:Int32">[io_TransactionNumber]</OutArgument>
                            </Assign.To>
                            <Assign.Value>
                              <InArgument x:TypeArguments="x:Int32">[io_TransactionNumber+1]</InArgument>
                            </Assign.Value>
                          </Assign>
                          <Assign sap2010:WorkflowViewState.IdRef="Assign_8">
                            <Assign.To>
                              <OutArgument x:TypeArguments="x:Int32">[io_RetryNumber]</OutArgument>
                            </Assign.To>
                            <Assign.Value>
                              <InArgument x:TypeArguments="x:Int32">0</InArgument>
                            </Assign.Value>
                          </Assign>
                        </Sequence>
                        <FlowStep.Next>
                          <FlowStep x:Name="__ReferenceID7" sap2010:WorkflowViewState.IdRef="FlowStep_26">
                            <Sequence sap2010:Annotation.AnnotationText="Set values for Status and Remarks fields and write current transaction status in the Status file." DisplayName="Write Status" sap2010:WorkflowViewState.IdRef="Sequence_8">
                              <Assign sap2010:Annotation.AnnotationText="Status is put in the second column (index 1)." DisplayName="Assign Status" sap2010:WorkflowViewState.IdRef="Assign_35">
                                <Assign.To>
                                  <OutArgument x:TypeArguments="x:Object">[StatusRow(1)]</OutArgument>
                                </Assign.To>
                                <Assign.Value>
                                  <InArgument x:TypeArguments="x:Object">[Status]</InArgument>
                                </Assign.Value>
                              </Assign>
                              <Assign sap2010:Annotation.AnnotationText="Remarks is the third field in the status row (index 2)." DisplayName="Assign Remarks" sap2010:WorkflowViewState.IdRef="Assign_36">
                                <Assign.To>
                                  <OutArgument x:TypeArguments="x:Object">[StatusRow(2)]</OutArgument>
                                </Assign.To>
                                <Assign.Value>
                                  <InArgument x:TypeArguments="x:Object">[Remarks]</InArgument>
                                </Assign.Value>
                              </Assign>
                              <ui:AddDataRow ArrayRow="{x:Null}" DataRow="[StatusRow]" DataTable="[TransactionStatus]" DisplayName="Add Data Row - Status" sap2010:WorkflowViewState.IdRef="AddDataRow_3" />
                              <Flowchart sap2010:Annotation.AnnotationText="Status file can be in Excel or CSV format.&#xA;&#xA;The TransactionStatus data table should contain only one row, the status for current transaction.&#xA;&#xA;Because the headers are not written if the data table is empty, we write the headers together with the first row, for transaction 1, first try." DisplayName="Write to Status File" sap2010:WorkflowViewState.IdRef="Flowchart_7">
                                <Flowchart.StartNode>
                                  <FlowDecision x:Name="__ReferenceID0" Condition="[&quot;Excel&quot; = in_Config(&quot;StatusFileFormat&quot;).ToString]" DisplayName="Excel format?" sap2010:WorkflowViewState.IdRef="FlowDecision_12">
                                    <FlowDecision.True>
                                      <FlowDecision x:Name="__ReferenceID1" Condition="[FirstStatus]" DisplayName="First status?" sap2010:WorkflowViewState.IdRef="FlowDecision_10">
                                        <FlowDecision.True>
                                          <FlowStep x:Name="__ReferenceID2" sap2010:WorkflowViewState.IdRef="FlowStep_29">
                                            <ui:WriteRange AddHeaders="True" DataTable="[TransactionStatus]" DisplayName="Write Range - write first row and the headers" sap2010:WorkflowViewState.IdRef="WriteRange_1" SheetName="Status" StartingCell="A1" WorkbookPath="[in_StatusFileName]" />
