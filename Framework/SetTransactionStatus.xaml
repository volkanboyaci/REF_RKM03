                                  <InArgument x:TypeArguments="x:String" x:Key="logF_TransactionExecutionTime">[TransactionExecutionTime]</InArgument>
                                </ui:AddLogFields.Fields>
                              </ui:AddLogFields>
                              <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Error" Message="[in_Config(&quot;LogMessage_BusinessRuleException&quot;).ToString + in_BusinessRuleException.Message]" />
                              <ui:RemoveLogFields DisplayName="Remove Transaction log fields" sap2010:WorkflowViewState.IdRef="RemoveLogFields_6">
                                <ui:RemoveLogFields.Fields>
