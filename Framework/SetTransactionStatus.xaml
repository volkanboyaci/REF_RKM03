  <Flowchart sap2010:Annotation.AnnotationText="This workflow sets the status of current transaction in the Status file and Logs that status and details in extra Logging Fields. &#xA;The Status file consists of Transaction Number, Tranaction Status, Remarks (details in case of exception) and input fields for current transaction.&#xA;The flowchart branches out into the three possible Transaction Statuses: Success, Business Exception and Application Exception. &#xA;&#xA;After that we log the result of the transaction within custom log fields to make it easier to search for within results. &#xA;&#xA;This workflow is also where incrementing of the io_TransactionNumber variable takes place. If we have an application exception and our MaxRetryNumber has not been reached, we increment the io_RetryNumber variable and not the io_TransactionNumber variables. This is done in the Robot Retry flowchart, which manages the retry mechanism of the framework and which is part of the &quot;Handle System Error&quot; sequence." DisplayName="SetTransactionStatus" sap2010:WorkflowViewState.IdRef="Flowchart_2">
    <Flowchart.Variables>
      <Variable x:TypeArguments="x:String" Name="Status" />
      <Variable x:TypeArguments="x:String" Name="Remarks" />
      <Variable x:TypeArguments="x:Boolean" Name="FirstStatus" />
      <Variable x:TypeArguments="sd:DataRow" Name="StatusRow" />
      <Variable x:TypeArguments="sd:DataTable" Name="TransactionStatus" />
      <Variable x:TypeArguments="x:String" Name="TransactionExecutionTime" />
    </Flowchart.Variables>
    <Flowchart.StartNode>
      <FlowStep x:Name="__ReferenceID25" sap2010:WorkflowViewState.IdRef="FlowStep_28">
        <Assign sap2010:Annotation.AnnotationText="We need to add hearders for first status that we write in the file, then we use append range for all other statuses." DisplayName="Assign FirstTransactionEver" sap2010:WorkflowViewState.IdRef="Assign_39">
          <Assign.To>
            <OutArgument x:TypeArguments="x:Boolean">[FirstStatus]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:Boolean">[1 = io_TransactionNumber AND 0 = io_RetryNumber]</InArgument>
          </Assign.Value>
        </Assign>
        <FlowStep.Next>
          <FlowStep x:Name="__ReferenceID24" sap2010:WorkflowViewState.IdRef="FlowStep_27">
            <Sequence sap2010:Annotation.AnnotationText="Create here the new status row for the current transaction. Status and Remarks fields are set later." DisplayName="Create Status row" sap2010:WorkflowViewState.IdRef="Sequence_9">
              <Sequence.Variables>
                <Variable x:TypeArguments="s:String[]" Name="ColumnsToExclude" />
              </Sequence.Variables>
              <Assign sap2010:Annotation.AnnotationText="We might want to exclude some columns from the status file. The list is defined in config file." DisplayName="Assign ColumnsToExclude" sap2010:WorkflowViewState.IdRef="Assign_40">
                <Assign.To>
                  <OutArgument x:TypeArguments="s:String[]">[ColumnsToExclude]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="s:String[]">[in_Config("StatusFileExcludeColumns").ToString.Split({","}, StringSplitOptions.RemoveEmptyEntries)]</InArgument>
                </Assign.Value>
              </Assign>
              <Assign sap2010:Annotation.AnnotationText="Status for current transaction is appended to the Status file using the Append Range activity which requires a Data Table as input - TransactionStatus." DisplayName="Assign TransactionStatus" sap2010:WorkflowViewState.IdRef="Assign_43">
                <Assign.To>
                  <OutArgument x:TypeArguments="sd:DataTable">[TransactionStatus]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="sd:DataTable">[in_StatusFileStructure.Clone]</InArgument>
                </Assign.Value>
              </Assign>
              <Assign sap2010:Annotation.AnnotationText="Create a new status row with the same structure as status file." DisplayName="Assign StatusRow" sap2010:WorkflowViewState.IdRef="Assign_41">
                <Assign.To>
                  <OutArgument x:TypeArguments="sd:DataRow">[StatusRow]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="sd:DataRow">[TransactionStatus.NewRow]</InArgument>
                </Assign.Value>
              </Assign>
              <ui:ForEach x:TypeArguments="sd:DataColumn" CurrentIndex="{x:Null}" DisplayName="For Each" sap2010:WorkflowViewState.IdRef="ForEach`1_2" Values="[in_TransactionItem.Table.Columns]">
                <ui:ForEach.Body>
                  <ActivityAction x:TypeArguments="sd:DataColumn">
                    <ActivityAction.Argument>
                      <DelegateInArgument x:TypeArguments="sd:DataColumn" Name="Column" />
                    </ActivityAction.Argument>
                    <If Condition="[Not ColumnsToExclude.Contains(Column.ColumnName)]" sap2010:WorkflowViewState.IdRef="If_2">
                      <If.Then>
                        <Assign DisplayName="Assign StatusRow from input file" sap2010:WorkflowViewState.IdRef="Assign_42">
                          <Assign.To>
                            <OutArgument x:TypeArguments="x:Object">[StatusRow(Column.ColumnName)]</OutArgument>
                          </Assign.To>
                          <Assign.Value>
                            <InArgument x:TypeArguments="x:Object">[in_TransactionItem(Column)]</InArgument>
                          </Assign.Value>
                        </Assign>
                      </If.Then>
                    </If>
                  </ActivityAction>
                </ui:ForEach.Body>
              </ui:ForEach>
              <Assign sap2010:Annotation.AnnotationText="First item in the status array is the transaction number." DisplayName="Assign Row's TransactionNumber" sap2010:WorkflowViewState.IdRef="Assign_37">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:Object">[StatusRow(0)]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:Object">[io_TransactionNumber]</InArgument>
                </Assign.Value>
              </Assign>
            </Sequence>
            <FlowStep.Next>
              <FlowStep x:Name="__ReferenceID26" sap2010:WorkflowViewState.IdRef="FlowStep_33">
                <Sequence sap2010:Annotation.AnnotationText="Get the Transaction Execution time from the provided Stopwatch object." DisplayName="Get the execution time" sap2010:WorkflowViewState.IdRef="Sequence_10">
                  <InvokeMethod sap2010:Annotation.AnnotationText="Stops measuring elapsed time." DisplayName="Invoke Method Stop" sap2010:WorkflowViewState.IdRef="InvokeMethod_1" MethodName="Stop">
                    <InvokeMethod.TargetObject>
                      <InArgument x:TypeArguments="sd1:Stopwatch">[in_TransactionExecutionWatch]</InArgument>
                    </InvokeMethod.TargetObject>
                  </InvokeMethod>
                  <Assign sap2010:Annotation.AnnotationText="Gets the Transaction Execution time in seconds with 7 digits precision." DisplayName="Assign TransactionExecutionTime" sap2010:WorkflowViewState.IdRef="Assign_44">
                    <Assign.To>
                      <OutArgument x:TypeArguments="x:String">[TransactionExecutionTime]</OutArgument>
                    </Assign.To>
                    <Assign.Value>
                      <InArgument x:TypeArguments="x:String">[String.Format("{0:0.0000000}", CDbl(in_TransactionExecutionWatch.ElapsedMilliseconds)/1000)]</InArgument>
                    </Assign.Value>
                  </Assign>
                </Sequence>
                <FlowStep.Next>
                  <FlowDecision x:Name="__ReferenceID19" Condition="[in_BusinessRuleException is Nothing and in_SystemError is Nothing]" DisplayName="Success" sap2010:WorkflowViewState.IdRef="FlowDecision_1">
                    <FlowDecision.True>
                      <FlowStep x:Name="__ReferenceID23" sap2010:WorkflowViewState.IdRef="FlowStep_10">
                        <Sequence sap2010:Annotation.AnnotationText="This Transition is picked up if we have no exception, of any type.&#xA;" DisplayName="Success" sap2010:WorkflowViewState.IdRef="Sequence_6">
                          <Assign DisplayName="Assign Status" sap2010:WorkflowViewState.IdRef="Assign_25">
                            <Assign.To>
                              <OutArgument x:TypeArguments="x:String">[Status]</OutArgument>
